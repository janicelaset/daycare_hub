<div class="panel-daycare-edit">
  <div class="panel panel-default">
    <a id="daycare-edit-name" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseName" aria-expanded="true" aria-controls="collapseName" class="collapsed">
      <div class="panel-heading" role="tab" id="headingOne">
        <h4 class="panel-title">
          Name
          <span class="glyphicon glyphicon-pencil daycare-edit-glyph pull-right"></span>
        </h4>
      </div>
    </a>
    <div id="collapseName" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
      <div class="panel-body" id="daycare-name-form">
        <%= render 'daycares/form' %>
      </div>
    </div>
  </div>
</div>
