<div id="daycare-profile-page">

  <div class="navbar navbar-fixed-top navbar-sub" role="navigation" data-0="line-height:100px; height:100px; background-color:rgba(0,0,0,0.3);" data-300="line-height:60px; height:60px; background-color:rgba(0,0,0,0.3);">
     <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="fa fa-bars color-white"></span>
        </button>
        <h1><a class="navbar-brand" href="/" data-0="line-height:90px;" data-300="line-height:50px;"><span class="name"><%= @daycare.name %></span></a></h1>
      </div>
      <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav" data-0="margin-top:20px;" data-300="margin-top:5px;">
          <li class="active"><a href="/">Home</a></li>
          <li><a href="#section-about">About</a></li>
          <li><a href="#section-works">Portfolio</a></li>
          <li><a href="#section-contact">Contact</a></li>
        </ul>
      </div><!--/.navbar-collapse -->
    </div>
  </div>
<div class="header-profile">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="intro-text">
          <span class="name"><%= @daycare.name %></span>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="container">
  <%= render 'images' %>
  <%= render 'summary' %>
  <%= render 'programs/profile_programs' %>
  <%= render 'teachers/profile_teachers' %>
  <%= render 'certifications/profile_certifications' %>
  <%= render 'additionals/profile_additionals' %>
  <%= render 'address' %>
  <%= render 'contact' %>
</div>
</div>

<script src="<%= asset_path('js/modernizr-2.6.2-respond-1.1.0.min.js') %>"></script>
<script src="<%= asset_path('js/jquery.js') %>"></script>
<script src="<%= asset_path('js/jquery.easing.1.3.js') %>"></script>
<script src="<%= asset_path('js/jquery.isotope.min.js') %>"></script>
<script src="<%= asset_path('js/jquery.nicescroll.min.js') %>"></script>
<script src="<%= asset_path('js/fancybox/jquery.fancybox.pack.js') %>"></script>
<script src="<%= asset_path('js/skrollr.min.js') %>"></script>
<script src="<%= asset_path('js/jquery.scrollTo-1.4.3.1-min.js') %>"></script>
<script src="<%= asset_path('js/jquery.localscroll-1.2.7-min.js') %>"></script>
<script src="<%= asset_path('js/stellar.js') %>"></script>
<script src="<%= asset_path('js/jquery.appear.js') %>"></script>
<script src="<%= asset_path('js/validate.js') %>"></script>
<script src="<%= asset_path('js/main.js') %>"></script>
