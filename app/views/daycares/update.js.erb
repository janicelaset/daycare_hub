
<% if @wizard == true %>
alert("wizard submit");
  <% if @daycare.errors.messages.any? %>
    $('#wizard-name-form').html('<%= j render 'daycares/form' %>')
  <% else %>
    window.location = "<%= j (wizard_name_user_daycare_path(@user, @daycare)) %>"
