<div class="panel-daycare-edit">
  <div class="panel panel-default">
    <a id="daycare-add-image" class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
      <div class="panel-heading" role="tab" id="headingFour">
        <h4 class="panel-title">
          <span class="glyphicon glyphicon-plus daycare-add-glyph pull-right"></span>
          Images
        </h4>
      </div>
    </a>
    <div id="collapseFour" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingFour">
      <div class="panel-body">
        <span id="image-add-list">
          <%= render 'images/image_list' %>
        </span>
        <h4>Add an Image</h4>
        <span id="image-add-form">
          <%= render "images/form" %>
        </span>
      </div>
    </div>
  </div>
</div>
