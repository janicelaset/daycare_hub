$('#edit-image-<%= @image.id %>').slideUp(350, function() {
  $("#daycare-image-<%= @image.id %>").html("<%= j image_tag @image.picture.url(:thumb) %>");
  // $("#image-add-list").html("<%= j render 'images/image_list' %>")
  $('#panel-name-image-<%= @image.id %>').text('<%= @image.picture_file_name %>');

  $('#imageList').sortable({
      axis: 'y',
      panel: '.panel-heading',
      update: function() {
        $.post($(this).data('update-url'), $(this).sortable('serialize'))
      }
    }
  );
});
