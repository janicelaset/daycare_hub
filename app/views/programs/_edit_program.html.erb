<div class="panel-daycare-edit">
  <div class="panel panel-default">
    <a id="daycare-edit-program" class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseProgram" aria-expanded="false" aria-controls="collapseProgram">
      <div class="panel-heading" role="tab" id="headingSix">
        <h4 class="panel-title">
          <div id="program-add-edit-glyph">
            <%= render 'programs/add_edit_glyph' %>
          </div>
          Programs
        </h4>
      </div>
    </a>
    <div id="collapseProgram" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingSix">
      <div class="panel-body">
        <span id="program-edit-list">
          <%= render 'programs/program_list' %>
        </span>
        <h4>Add a Program</h4>
        <span id="program-edit-form">
          <%= render 'programs/form' %>
        </span>
      </div>
    </div>
  </div>
</div>
