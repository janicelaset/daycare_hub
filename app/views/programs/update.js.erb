$('#program-edit-modal').modal("hide");

$('#panel-name-program-<%= @program.id %>').text('<%= @program.name %>')
$('#header-name-program-<%= @program.id %>').text('<%= @program.name %>')
$('#program-age-from-<%= @program.id %>').text('<%= @program.age_from %>')
<% if @program.age_from_units == @program.age_to_units %>
  $('#program-age-from-units-<%= @program.id %>').text('')
<% else %>
  $('#program-age-from-units-<%= @program.id %>').text('<%= @program.age_from_units %>')
<% end %>
$('#program-age-to-<%= @program.id %>').text('<%= @program.age_to %>')
<% if @program.available == true %>
  $('#program-availability-<%= @program.id %>').text('yes')
<% else %>
  $('#program-availability-<%= @program.id %>').text('none')
<% end %>
$('#program-age-to-units-<%= @program.id %>').text('<%= @program.age_to_units %>')
$('#program-description-<%= @program.id %>').html('<%= j simple_format h @program.description %>')
$("#program-image<%= @program.id %>").html("<%= j image_tag @program.picture.url(:small) %>");

$('#programList').sortable({
    axis: 'y',
    panel: '.panel-heading',
    update: function() {
      $.post($(this).data('update-url'), $(this).sortable('serialize'))
    }
  }
);
